@Library('ised-cicd-lib') _

pipeline {
	agent {
       	label 'maven'
   	}
   	
    options {
        disableConcurrentBuilds()
    }
    
	environment {
		MVN_DEBUG_LOGGING = "true"
	}
    
    stages {
		stage('release') {
			steps {
				script{
					release.releaseMavenLibrary()
				}
			}
		}
    }
}
